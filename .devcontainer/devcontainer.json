{
	"name": "wordpress-go-proxy",
	"image": "mcr.microsoft.com/devcontainers/base:bullseye",
	"containerEnv": {
		"SHELL": "/bin/zsh"
	},
	"features": {
		"ghcr.io/devcontainers/features/go:1": {},
		"ghcr.io/devcontainers/features/terraform:1": {
			"version": "1.10.5",
			"tflint": "latest"
		},
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"hashicorp.terraform",
				"github.copilot",
				"github.vscode-github-actions",
				"hashicorp.hcl"
			]
		}
	},
	"remoteUser": "vscode"
}
